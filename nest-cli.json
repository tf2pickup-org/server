{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "documents/default/*.md",
        "outDir": "dist/src"
      },
      {
        "include": "game-configs/configs/*.cfg",
        "outDir": "dist/src"
      },
      {
        "include": "../client/*",
        "outDir": "dist/client"
      }
    ],
    "watchAssets": true
  }
}
